<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fady.quiz" >
    <uses-permission android:name="android.permission.INTERNET">

    </uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.LOGIN" />



                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".student"
            android:label="@string/student_tabed"
            android:logo="@drawable/ic_launcher" >
            <intent-filter>
                <action android:name="android.intent.action.SENDTO" />

                <data android:scheme="mailto" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".quiz"
            android:label="@string/title_activity_quiz" >
        </activity>
        <activity
            android:name=".instructor"
            android:label="@string/title_activity_instructor" >
        </activity>
        <activity
            android:name=".finish"
            android:label="@string/title_activity_finish" >
        </activity>
        <activity
            android:name=".makeQuiz"
            android:label="@string/title_activity_makeQuiz" >
        </activity>

<activity android:name="org.achartengine.GraphicalActivity"></activity>
        <activity android:name=".test"></activity>

    </application>

</manifest>
